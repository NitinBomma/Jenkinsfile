pipeline {
    agent any

    stages {
        stage('Hello') {
            steps {
                echo 'Hello World'
                echo 'i am new to '
            }
        }
        stage('Build') {
            steps {
                echo 'Building'
            }
        }
        stage('Deploy') {
            steps {
                echo 'Deploying'
            }
        }
        stage('test') {
            steps {
                echo 'Testing'
            }
        }
        stage('release') {
            steps {
                echo 'releasing'
            }
        }
    }
        
}
